/**
 * @param {number[]} nums
 * @param {number} target
 * @return {number[]}
 */
var twoSum = function(nums, target) {
    for (let i = 0; i <nums.length; i++){
        var a = nums[i];
        for (let j = i+1; j < nums.length; j++){
            var b = nums[j];
            if (a+b == target) return [i, j]
        }
    }
};
